//condition2_传入的除拼音码之外的查询条件   functionname拼音码调用时，需要级联调用的方法名称
function smanPromptList(objInputId,url,outObj,condition2_,functionname){

        var objouter=document.getElementById("__smanDisp") //显示的DIV对象
        var objInput = getBodyDom(objInputId);//document.getElementById(objInputId); //文本框对象
        
        if (objInput.value ==""){
    		getBodyDom(outObj).value ="";
    	} 
        
        var condition2Value_ = "";
        if(condition2_ != "" && condition2_ != undefined)
        	condition2Value_ = getBodyDom(condition2_);
        var selectedIndex=-1;
        var intTmp; //循环用的:)
		var int; 
		var outName;
		var strInputLength;
        if (objInput==null) {return ;}
            //文本框失去焦点
            objInput.onblur=function(){
                objouter.style.display='none';
            }
			     objInput.onfocus=checkAndShow;
		   //文本框按键抬起
                 objInput.onkeyup=checkKeyCode;
			//文本框按键按下
            function checkKeyCode(evt){
				
                            evt = evt || window.event;
                var ie = (document.all)? true:false//document.all表示document中所有对象的集合，只有ie支持此属性，因此也用来判断浏览器的种类
               
                if (ie){
                    var keyCode=evt.keyCode
                    if (keyCode==40||keyCode==38){ var isUp=false;if(keyCode==40) isUp=true ;chageSelection(isUp)}
					
					else if (keyCode==13){
						if (objInput.value !=""){
							outSelection(selectedIndex);
						} else {
							getBodyDom(outObj).value ="";
						}
					}
					else if(strInputLength!=objInput.value){checkAndShow(evt)}
					else{checkAndShow(evt)}
                }
				else{checkAndShow(evt)}
                
				divPosition(evt)
            }
            function checkAndShow(evt){
                        var strInput = objInput.value;
                        var condition2 = "";
                        if(condition2Value_ != "" && condition2Value_ != undefined)
                        	condition2 = condition2Value_.value;
                        if (strInput !=""){
                        	
							strInputLength=strInput;
							outName=outObj;
							date=new Date();
							if(condition2 != "" && condition2 != undefined){
								getUrl(url+strInput+"&condition2="+condition2+"&"+date);
							}else{
								getUrl(url+strInput+"&"+date);
							}
							khMc = http_request.responseText;
							dearray(khMc);
							divPosition(evt);
                            selectedIndex=-1;
                            objouter.innerHTML ="";
							if (arr.length>20) { int=21;}else {int=arr.length}
						   		for (intTmp=1;intTmp<int;intTmp++){
						   			//objouter.innerHTML+="<iframe style='position:absolute;z-index:-1;border:0px;background:#E8F7EB;' frameborder='no' border='0' marginwidth='0' marginheight='0' scrolling='no'  width='1' height='20'></iframe>"
									addOption(arrList[intTmp]);
                            	}
                            	objouter.style.display="block";
                        	}else{
                            	objouter.style.display='none';
                            	getBodyDom(outObj).value ="";
                    		}
                    function addOption(value){
                      	kh=value;
                      	var t_hid_ids = "";
                      	if(value.length>1){
                      		t_hid_ids=value[1].split("#")[0];
	                    	if (value[1].split("#").length==2) {
	                    		value[1] = value[1].split("#")[1];
	                    	}
							if(functionname != null && functionname != undefined && functionname != "")
							 objouter.innerHTML +="<div onmouseover=\"this.className='sman_selectedStyle'\" onmouseout=\"this.className=''\" onmousedown=\"getBodyDom('"+objInputId+"').value='"+value[0]+"';id='"+value[1]+"';getBodyDom('"+outName+"').value='"+t_hid_ids+"';"+functionname+";if (funcname!=null) {eval('"+funcname+"')};\"  style='width:auto;height:22px;text-align:left'><div align='right' style='display:inline'>"+value[0]+"</div>&nbsp;&nbsp;</div>";   
                      		else
                      		 objouter.innerHTML +="<div onmouseover=\"this.className='sman_selectedStyle'\" onmouseout=\"this.className=''\" onmousedown=\"getBodyDom('"+objInputId+"').value='"+value[0]+"';id='"+value[1]+"';getBodyDom('"+outName+"').value='"+t_hid_ids+"';if (funcname!=null) {eval('"+funcname+"')};\"  style='width:auto;height:22px;text-align:left'><div align='right' style='display:inline'>"+value[0]+"</div>&nbsp;&nbsp;</div>";   
                      	}
                    }
            }
            function chageSelection(isUp){
                if (objouter.style.display=='none'){
                    objouter.style.display='';
                }else{
                    if (isUp)
                        selectedIndex++
                    else
                        selectedIndex--
                }
                var maxIndex = objouter.children.length-1;
                if (selectedIndex<0){selectedIndex=0}
                if (selectedIndex>maxIndex) {selectedIndex=maxIndex}
                for (intTmp=0;intTmp<=maxIndex;intTmp++){

                    if (intTmp==selectedIndex){
                        objouter.children[intTmp].className="sman_selectedStyle";
                    }else{
                        objouter.children[intTmp].className="";
                    }
                }
            }
            function outSelection(Index){
				
               objInput.value = objouter.children[Index].innerText;
				getBodyDom(outName).value=arrList[Index+1][1];
				objouter.style.display='none';
				if(functionname != null && functionname != undefined && functionname != ""){
					var functionnames = functionname.split(";");
					for(var i=0;i<functionnames.length;i++){
						eval(functionnames[i]);
					}
				}
				if (outName=="hidKhmc"){
					if (getBodyDom("txtWzzl")){
						getBodyDom("txtWzzl").focus();
					}
					
				}
				else{
					if (getBodyDom("txtHtl")){
						getBodyDom("txtHtl").focus();
					}
				}
				
            }
            function divPosition(evt){
                var left = 0;
                var top  = 0;                                
                var e = objInput;
                while (e.offsetParent){
                	left += e.offsetLeft + (e.currentStyle?(parseInt(e.currentStyle.borderLeftWidth)).NaN0():0);
                    top  += e.offsetTop  + (e.currentStyle?(parseInt(e.currentStyle.borderTopWidth)).NaN0():0);
                    e = e.offsetParent;
               	}
				
                left += e.offsetLeft + (e.currentStyle?(parseInt(e.currentStyle.borderLeftWidth)).NaN0():0);
                top  += e.offsetTop  + (e.currentStyle?(parseInt(e.currentStyle.borderTopWidth)).NaN0():0);
                objouter.style.top  = (top  + objInput.clientHeight) + 'px' ;
                objouter.style.left = left + 'px' ; 
               // objouter.style.width= objInput.clientWidth;
				
				
            }
    }
    
    document.write("<div id='__smanDisp' style='z-index:9999999999999999999999;position:absolute;display:none;background:#E8F7EB;border: 1px solid #CCCCCC;font-size:15px;cursor: default;' > </div>");
    document.write("<style>.sman_selectedStyle{background-Color:#102681;color:#FFFFFF}</style>");
   
    function getAbsoluteHeight(ob){
        return ob.offsetHeight
    }
    function getAbsoluteWidth(ob){
        return ob.offsetWidth
    }
    function getAbsoluteLeft(ob){
        var mendingLeft = ob .offsetLeft;
        while( ob != null && ob.offsetParent != null && ob.offsetParent.tagName != "BODY" ){
            mendingLeft += ob .offsetParent.offsetLeft;
            mendingOb = ob.offsetParent;
        }
        return mendingLeft ;
    }
    function getAbsoluteTop(ob){
        var mendingTop = ob.offsetTop;
        while( ob != null && ob.offsetParent != null && ob.offsetParent.tagName != "BODY" ){
            mendingTop += ob .offsetParent.offsetTop;
            ob = ob .offsetParent;
        }
        return mendingTop ;
    }
    
    function getBodyDom(id) {
    	if (isNvTab=='1') {
    		return $('#'+id,navTab.getCurrentPanel())[0];
    	} else if(isNvTab=='2') {
    		return $('#'+id,$.pdialog.getCurrent())[0];
    	}else{
    	    return document.getElementById(id);
    	}
    }
    
Number.prototype.NaN0 = function()
{
    return isNaN(this)?0:this;
}
</script>

 
